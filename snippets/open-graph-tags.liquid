{%- comment %}<locksmith:3b9c>{% endcomment -%}
  {%- assign locksmith_17c4_product__price = nil %}{% for variant in product.variants %}{% include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: variant, locksmith_subject_parent: product %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif %}{% include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: variant, locksmith_subject_parent: product %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif %}{% if locksmith_17c4_product__price == nil %}{% assign locksmith_17c4_product__price = variant.price %}{% endif %}{% endfor -%}
{%- comment %}</locksmith:3b9c>{% endcomment -%}

{% if template contains 'product' %}
  {%- comment %}<locksmith:5af1>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:type" content="product" />{% endif -%}
    {%- comment %}original: <meta property="og:type" content="product" />{%- endcomment %}
  {%- comment %}</locksmith:5af1>{% endcomment -%}
  {%- comment %}<locksmith:7110>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:title" content="{{ product.title | strip_html | escape }}" />{% endif -%}
    {%- comment %}original: <meta property="og:title" content="{{ product.title | strip_html | escape }}" />{%- endcomment %}
  {%- comment %}</locksmith:7110>{% endcomment -%}
  {% for image in product.images %}
  {%- comment %}<locksmith:861a>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: image, locksmith_subject_parent: product %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif -%}
  {%- comment %}</locksmith:861a>{% endcomment -%}
  {%- comment %}<locksmith:32e0>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:image" content="http:{{ image | img_url: 'grande' }}" />{% endif -%}
    {%- comment %}original: <meta property="og:image" content="http:{{ image | img_url: 'grande' }}" />{%- endcomment %}
  {%- comment %}</locksmith:32e0>{% endcomment -%}
  {%- comment %}<locksmith:6a33>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:image:secure_url" content="https:{{ image | img_url: 'grande' }}" />{% endif -%}
    {%- comment %}original: <meta property="og:image:secure_url" content="https:{{ image | img_url: 'grande' }}" />{%- endcomment %}
  {%- comment %}</locksmith:6a33>{% endcomment -%}
  {% endfor %}
  {%- comment %}<locksmith:21d6>{% endcomment -%}
    {%- include 'locksmith-variables' %}{% if locksmith_access_granted %}<meta property="og:price:amount" content="{{ locksmith_17c4_product__price | money_without_currency }}" />{% endif -%}
    {%- comment %}original: <meta property="og:price:amount" content="{{ product.price | money_without_currency }}" />{%- endcomment %}
  {%- comment %}</locksmith:21d6>{% endcomment -%}
  {%- comment %}<locksmith:cd9d>{% endcomment -%}
    {%- include 'locksmith-variables' %}{% if locksmith_access_granted %}<meta property="og:price:currency" content="{{ shop.currency }}" />{% endif -%}
    {%- comment %}original: <meta property="og:price:currency" content="{{ shop.currency }}" />{%- endcomment %}
  {%- comment %}</locksmith:cd9d>{% endcomment -%}
{% elsif template contains 'article' %}
  {%- comment %}<locksmith:b666>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:type" content="article" />{% endif -%}
    {%- comment %}original: <meta property="og:type" content="article" />{%- endcomment %}
  {%- comment %}</locksmith:b666>{% endcomment -%}
  {%- comment %}<locksmith:e034>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:title" content="{{ article.title | strip_html | escape }}" />{% endif -%}
    {%- comment %}original: <meta property="og:title" content="{{ article.title | strip_html | escape }}" />{%- endcomment %}
  {%- comment %}</locksmith:e034>{% endcomment -%}  
  {% assign img_tag = '<' | append: 'img' %}
  {% if article.content contains img_tag %}
    {% assign src = article.content | split: 'src="' %}
    {% assign src = src[1] | split: '"' | first | remove: 'https:' | remove: 'http:' %}
    {% if src %}
      {%- comment %}<locksmith:16d8>{% endcomment -%}
        {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:image" content="http:{{ src }}" />{% endif -%}
        {%- comment %}original: <meta property="og:image" content="http:{{ src }}" />{%- endcomment %}
      {%- comment %}</locksmith:16d8>{% endcomment -%}
      {%- comment %}<locksmith:1a7e>{% endcomment -%}
        {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:image:secure_url" content="https:{{ src }}" />{% endif -%}
        {%- comment %}original: <meta property="og:image:secure_url" content="https:{{ src }}" />{%- endcomment %}
      {%- comment %}</locksmith:1a7e>{% endcomment -%}
    {% endif %}
  {% endif %}
{% else %}
    {%- comment %}<locksmith:9232>{% endcomment -%}
      {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:image" content="{{ 'facebook-share2.jpg' | asset_url }}" />{% endif -%}
      {%- comment %}original: <meta property="og:image" content="{{ 'facebook-share2.jpg' | asset_url }}" />{%- endcomment %}
    {%- comment %}</locksmith:9232>{% endcomment -%}
    {%- comment %}<locksmith:c58b>{% endcomment -%}
      {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:image:secure_url" content="{{ 'facebook-share2.jpg' | asset_url }}" />{% endif -%}
      {%- comment %}original: <meta property="og:image:secure_url" content="{{ 'facebook-share2.jpg' | asset_url }}" />{%- endcomment %}
    {%- comment %}</locksmith:c58b>{% endcomment -%}
  {%- comment %}<locksmith:b0f0>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:title" content="{{ page_title | escape }}" />{% endif -%}
    {%- comment %}original: <meta property="og:title" content="{{ page_title | escape }}" />{%- endcomment %}
  {%- comment %}</locksmith:b0f0>{% endcomment -%}
  {%- comment %}<locksmith:f5c4>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:type" content="website" />{% endif -%}
    {%- comment %}original: <meta property="og:type" content="website" />{%- endcomment %}
  {%- comment %}</locksmith:f5c4>{% endcomment -%}
{% endif %}

{% if page_description %}
{%- comment %}<locksmith:95ec>{% endcomment -%}
  {%- include 'locksmith-variables', locksmith_hiding_scope: 'resource' %}{% if locksmith_transparent %}<meta property="og:description" content="{{ page_description }}" />{% endif -%}
  {%- comment %}original: <meta property="og:description" content="{{ page_description }}" />{%- endcomment %}
{%- comment %}</locksmith:95ec>{% endcomment -%}
{% endif %}
<meta property="og:url" content="{{ canonical_url }}" />
<meta property="og:site_name" content="{{ shop.name }}" />