{%- comment %}<locksmith:da40>{% endcomment -%}
  {%- assign locksmith_69bf_wcp_discount__price = nil %}{% assign locksmith_69bf_wcp_discount__price_min = nil %}{% assign locksmith_69bf_wcp_discount__price_max = nil %}{% assign locksmith_69bf_wcp_discount__compare_at_price = nil %}{% assign locksmith_69bf_wcp_discount__compare_at_price_min = nil %}{% assign locksmith_69bf_wcp_discount__compare_at_price_max = nil %}{% for variant in wcp_discount.variants %}{% include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: variant, locksmith_subject_parent: wcp_discount %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif %}{% include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: variant, locksmith_subject_parent: wcp_discount %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif %}{% if locksmith_69bf_wcp_discount__price == nil %}{% assign locksmith_69bf_wcp_discount__price = variant.price %}{% endif %}{% if locksmith_69bf_wcp_discount__price_min == nil or variant.price < locksmith_69bf_wcp_discount__price_min %}{% assign locksmith_69bf_wcp_discount__price_min = variant.price %}{% endif %}{% if locksmith_69bf_wcp_discount__price_max == nil or locksmith_69bf_wcp_discount__price_max < variant.price %}{% assign locksmith_69bf_wcp_discount__price_max = variant.price %}{% endif %}{% if locksmith_69bf_wcp_discount__compare_at_price == nil %}{% assign locksmith_69bf_wcp_discount__compare_at_price = variant.compare_at_price %}{% endif %}{% if locksmith_69bf_wcp_discount__compare_at_price_min == nil or variant.compare_at_price < locksmith_69bf_wcp_discount__compare_at_price_min %}{% assign locksmith_69bf_wcp_discount__compare_at_price_min = variant.compare_at_price %}{% endif %}{% if locksmith_69bf_wcp_discount__compare_at_price_max == nil or locksmith_69bf_wcp_discount__compare_at_price_max < variant.compare_at_price %}{% assign locksmith_69bf_wcp_discount__compare_at_price_max = variant.compare_at_price %}{% endif %}{% endfor %}{% assign _69bf_safe_variants = '' %}{% for _variant in wcp_discount.variants %}{% include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: _variant, locksmith_subject_parent: wcp_discount %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif %}{% capture _69bf_safe_variants %}{% if _69bf_safe_variants != blank %}{{ _69bf_safe_variants }},{% endif %}{{ _variant | json }}{% endcapture %}{% endfor %}{% capture _69bf_safe_variants %}[{{ _69bf_safe_variants }}]{% endcapture %}{% assign _69bf_unsafe_variants = wcp_discount.variants | json %}{% assign _69bf_safe_options = wcp_discount.options | json %}{% assign _69bf_unsafe_options = wcp_discount.options | json %}{% for _option in wcp_discount.options %}{% assign _safe_values = '' %}{% for _value in _option.values %}{% include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: _value, locksmith_subject_parent: _option %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif %}{% capture _safe_values %}{% if _safe_values != blank %}{{ _safe_values }},{% endif %}{{ _value | json }}{% endcapture %}{% endfor %}{% capture _safe_values %}[{{ _safe_values }}]{% endcapture %}{% assign _unsafe_values = _option.values | json %}{% assign _unsafe_option = _option | json %}{% assign _safe_option = _unsafe_option | replace: _unsafe_values, _safe_values %}{% assign _69bf_safe_options = _69bf_safe_options | replace: _unsafe_option, _safe_option %}{% endfor %}{% assign _69bf_unsafe_options = wcp_discount.options | json -%}
{%- comment %}</locksmith:da40>{% endcomment -%}

{% assign wcp_discount_value = 0 %}
{% assign wcp_price_min_wo_compare = null %}
{% assign set_price = null %}
{% assign active_discounts = shop.metafields.wcp_active_discounts.wcp_active_discounts %}
{% assign active_discounts_array = active_discounts | split: ',' %}
{% assign wcp_metafields = null %}
{% assign wcp_customer_tag = null %}

{% assign wcp_compare_at_price = locksmith_69bf_wcp_discount__compare_at_price %}
{% assign wcp_compare_at_price_min = locksmith_69bf_wcp_discount__compare_at_price_min %}
{% assign wcp_compare_at_price_max = locksmith_69bf_wcp_discount__compare_at_price_max %}
{% assign wcp_price_min = locksmith_69bf_wcp_discount__price_min %}
{% assign wcp_price_max = locksmith_69bf_wcp_discount__price_max %}
{% assign wcp_price = locksmith_69bf_wcp_discount__price %}


{% if shop.metafields.wcp_status.wcp_status != 'disabled' and customer %}
  {% assign p_discount_value = 0 %}
  {% for discount_key in active_discounts_array %}
    {% assign key_split = discount_key | split: '-' %}
    {% assign key_split_length = key_split | size %}

    {% if key_split_length > 2 %}
      {% assign p_value = key_split.last | plus: 0 %}
      {% assign removeable_key = key_split.last | prepend: '-' %}
      {% assign p_key = discount_key | remove_first: removeable_key %}
    {% else %}
      {% assign p_key = key_split[0] %}
      {% assign p_value = key_split[1] | plus: 0 %}
    {% endif %}

    {% if customer.tags contains p_key %}
      {% assign discountgroup = shop.metafields.wcp_discountgroup[discount_key] %}
      {% if discountgroup contains 'entire' %}
        {% if p_value >= p_discount_value %}
          {% assign p_discount_value = p_value %}
          {% assign p_discount_tag = p_key %}
        {% endif %}

      {% elsif discountgroup contains 'collection' %}
        {% for c in wcp_discount.collections %}
          {%- comment %}<locksmith:fbe0>{% endcomment -%}
            {%- include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: c, locksmith_subject_parent: wcp_discount %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif -%}
          {%- comment %}</locksmith:fbe0>{% endcomment -%}
          {% if discountgroup contains c.id %}
          	{% if p_value >= p_discount_value %}
              {% assign p_discount_value = p_value %}
              {% assign p_discount_tag = p_key %}
            {% endif %}
          {% endif %}
        {% endfor %}

      {% elsif discountgroup contains 'product' %}
        {% if discountgroup contains wcp_discount.id %}
          {% if p_value >= p_discount_value %}
            {% assign p_discount_value = p_value %}
            {% assign p_discount_tag = p_key %}
          {% endif %}
        {% endif %}
      {% endif %}
    
{% endif %}
{% endfor %}

{% if customer.tags contains p_discount_tag %}
    {% assign wcp_customer_tag = p_discount_tag %}
    {% assign wcp_discount_value = p_discount_value | divided_by: 100.0 %}
  {% endif %}


{% assign wcp_discount_value = 1 | minus: wcp_discount_value %}
{% if wcp_discount_value < 1 %}
  {%- capture wcp_product_json -%}{%- comment %}<locksmith:2f4d>{% endcomment -%}{{- wcp_discount | json | replace: _69bf_unsafe_variants, _69bf_safe_variants | replace: _69bf_unsafe_options, _69bf_safe_options -}}{%- comment %}original: {{ wcp_discount | json }}{%- endcomment %}{%- comment %}</locksmith:2f4d>{% endcomment -%}{%- endcapture wcp_product_json -%}  
{% endif %}


{% assign wcp_price_min = locksmith_69bf_wcp_discount__price_max %}

{% assign raw_set_prices = wcp_discount.metafields.wcp_set_prices.wcp_set_prices %}

{% for variant in wcp_discount.variants %}
  {%- comment %}<locksmith:49e4>{% endcomment -%}
    {%- include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: variant, locksmith_subject_parent: wcp_discount %}{% if locksmith_transparent %}{% else %}{% continue %}{% endif -%}
  {%- comment %}</locksmith:49e4>{% endcomment -%}
  {% assign temp_set_price = 9999999 %}
   {% if raw_set_prices %}

    {% assign set_prices_array = raw_set_prices | split: "," %}
    {% for sp in set_prices_array %}
      {% assign sp_arr = sp | split: '--' %}
      {% assign sp_tag = sp_arr | first %}
      {% assign sp_var_arr = sp_arr | last | split: '^' %}

      {% if active_discounts contains sp_tag %}
        {% if customer.tags contains sp_tag %}  

          {% for value in sp_var_arr %}
            {% assign val_split = value | split: '-' %}
            {% assign variant_id = val_split | first | times: 1 %}
            {% if variant_id == variant.id %}
              {% assign meta_set_price = val_split | last |times: 1 %}
              {% if meta_set_price < temp_set_price  %}
                {% assign temp_set_price = meta_set_price %}
                {% assign v_discount_tag = sp_tag %}
              {% endif %}
            {% endif %}
          {% endfor %} 

        {% endif %}
      {% endif %}
    {% endfor %}
  {% endif %}


  {% if v_discount_tag and temp_set_price != 9999999 %}
    {% assign v_price = temp_set_price %}
    {% if v_price == null %}
     {% assign v_price = variant.price %}
    {% else %}
      {% assign set_price = v_price %}
      {% if set_price < wcp_price_min %}
        {% assign wcp_price_min = set_price %}
      {% endif %}
    {% endif %}

  {% else %}
      {% assign v_price = variant.price %}
  {% endif %}
  


    {% if v_price < wcp_price_min %}
      {% assign wcp_price_min = v_price %}
    {% endif %}

    {% if variant.compare_at_price == blank or variant.compare_at_price == 0 %}
      {% assign wcp_price_min_wo_compare = v_price %}
    {% endif %}


{% endfor %}

{% if wcp_discount_value < 1 or set_price %}

    {% if wcp_compare_at_price == blank  or wcp_compare_at_price == 0 or wcp_compare_at_price < wcp_price_min or shop.metafields.wcp_net.DiscountPriceRule  == '1' %}
        {% assign wcp_compare_at_price = locksmith_69bf_wcp_discount__price %}
    {% endif %}

    {% if wcp_compare_at_price_max == blank or wcp_compare_at_price_max == 0 %}
        {% assign wcp_compare_at_price_max = wcp_price_max %}
    {% endif %}
    {% assign wcp_price_max = wcp_compare_at_price_max | times: wcp_discount_value %}




    {% if wcp_compare_at_price_min == blank or wcp_compare_at_price_min == 0 or wcp_compare_at_price_min < wcp_price_min  %}
        {% assign wcp_compare_at_price_min = locksmith_69bf_wcp_discount__price %}
    {% endif %}


	{% if set_price == null %}

      {% assign wcp_price_min = wcp_compare_at_price_min | times: wcp_discount_value %}


      {% if wcp_price_min_wo_compare == null %}
        {% assign wcp_price_min = wcp_compare_at_price_min | times: wcp_discount_value %}
      {% else %}

          {% if  wcp_price_min_wo_compare < wcp_compare_at_price_min %}
            {% assign wcp_compare_at_price_min = wcp_price_min_wo_compare %}
            {% assign wcp_price_min = wcp_price_min_wo_compare | times: wcp_discount_value %}
          {% endif %}
      {% endif %}
	{% endif %}


    {% assign wcp_price = wcp_compare_at_price | times: wcp_discount_value %}
    {% if wcp_price > locksmith_69bf_wcp_discount__price %}
      {% assign wcp_price = locksmith_69bf_wcp_discount__price %}
    {% endif %}

	{% if set_price %}
      {% assign wcp_price = set_price %}
	{% endif %}

  	{% if wcp_price_min > locksmith_69bf_wcp_discount__price_min %}
		{% assign wcp_price_min = locksmith_69bf_wcp_discount__price_min %}
    {% endif %}

    {% if wcp_price_max > locksmith_69bf_wcp_discount__price_max %}
		{% assign wcp_price_max = locksmith_69bf_wcp_discount__price_max %}
  {% endif %}
{% endif %}
 {% assign wcp_price = wcp_price | floor %}
{% if show_js %}
<script>
  {% if wcp_metafields %}
    {% assign wcp_meta_arr = wcp_metafields | split: ',' %}
    {% for meta in wcp_meta_arr %}
      {% assign key_val = meta |split: '-' %}
      {% if key_val[1] %}
        window.wcp_metafields[{{key_val[0]}}] = {{key_val[1]}};
      {% endif %}
    {% endfor %}
  {% endif %}
  window.wcp_discount_value = {{wcp_discount_value}};
  window.wcp_customer_tag = "{{wcp_customer_tag}}";

</script>
{% endif %}
{% else %}
{% assign wcp_compare_at_price = locksmith_69bf_wcp_discount__compare_at_price %}
{% assign wcp_compare_at_price_min = locksmith_69bf_wcp_discount__compare_at_price_min %}
{% assign wcp_compare_at_price_max = locksmith_69bf_wcp_discount__compare_at_price_max %}
{% assign wcp_price_min = locksmith_69bf_wcp_discount__price_min %}
{% assign wcp_price_max = locksmith_69bf_wcp_discount__price_max %}
{% assign wcp_price = locksmith_69bf_wcp_discount__price %}
{% endif %}