<div class="fullimage-section">
  <div class='row'>
  	 {%- if section.settings.imagefull != blank -%}
    	<div class='col-md-12'>
            <img src="{{ section.settings.imagefull | img_url: 'master' }}">
        </div>
         {%- endif -%}
  </div>
  		<div class="fullimage-bottom">
            {%- if section.settings.imagebottom != blank -%}
                <img src="{{ section.settings.imagebottom | img_url: 'master' }}">
            {%- endif -%}
  		</div>
</div>



{% schema %}
  {
    "name": "Fullimage Section",
    "settings": [
		{
			"type": "image_picker",
          	"id": "imagefull",
          	"label": "imagefull"
