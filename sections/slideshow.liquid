{% comment %} 
Name: Slideshow
Version: 1.0
{% endcomment %}



{% schema %}

{
  "name": "Slideshow",
  "blocks": [
  {
    "type": "select",
    "name": "Add Item",
    "limit": 4,
    "settings": [
    {
      "type": "image_picker",
      "label": "Add Image",
      "id": "item-image"
    }, {
      "type": "text",
      "id": "title",
      "label": "Title"
    }, {
      "type": "html",
      "id": "heading",
      "label": "Heading"
    }, {
      "type": "radio",
      "id": "heading-position",
      "label": "Change heading alignment",
      "options": [
      {
        "value": "left",
        "label": "Left"
      }, {
        "value": "right",
        "label": "Right"
      }, {
        "value": "center",
        "label": "Center"
      }
      ]
    }, {
      "id": "btn-label",
      "type": "text",
      "label": "Button Label"
    }, {
      "id": "btn-link",
      "type": "url",
      "label": "Button link"
    }, {
      "type": "radio",
      "id": "btn-position",
      "label": "Change button alignment",
      "options": [
      {
        "value": "left",
        "label": "Left"
      }, {
        "value": "right",
        "label": "Right"
      }, {
        "value": "center",
        "label": "Center"
      }
      ]
    }
    ]
  }
  ],
  "presets": [
  {
    "name": "Slideshow",
    "category": "Layout"
  }
  ]
}

{% endschema %}

