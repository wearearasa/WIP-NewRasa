<div class="membership-block">
  <div class="image-topslide">
    {%- if section.settings.slightimagetop != blank -%}
    <img src="{{ section.settings.slightimagetop | img_url: 'master' }}">
    {%- endif -%}
  </div>
  {%- if section.settings.membership_topimg != blank -%}
    <div class="membership-topimg">
      <img src="{{ section.settings.membership_topimg | img_url: 'master' }}">
    </div>
  {%- endif -%}
  <div class="membership-section"></div>
  <div class="guarantee-image">
    {%- if section.settings.guaranteebottom != blank -%}
    <img src="{{ section.settings.guaranteebottom | img_url: 'master' }}">
    {%- endif -%} 
  </div>
  <div class="image-bottomslide">
    {%- if section.settings.slightimage != blank -%}
    <img src="{{ section.settings.slightimage | img_url: 'master' }}">
    {%- endif -%}
  </div>
</div>
